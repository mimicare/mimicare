{
  "name": "@mimicare/schema",
  "version": "0.0.1",
  "private": true,
  "main": "./client/index.js",
  "types": "./client/index.d.ts",
  "scripts": {
    "build": "yarn prisma:clean && prisma generate",
    "db:generate": "yarn prisma:clean && prisma generate",
    "prisma:clean": "rimraf prisma/generated",
    "db:migrate": "prisma migrate dev",
    "db:migrate:create": "prisma migrate dev --create-only",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:validate": "prisma validate",
    "db:format": "prisma format",
    "db:reset": "prisma migrate reset",
    "clean": "rimraf node_modules .turbo prisma/generated dist"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/pg": "^8.16.0",
    "prisma": "^7.1.0",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  }
}
